#include <stdio.h>
#include "system.h"
#include <stdlib.h>
#include "altera_avalon_pio_regs.h"

int main()
{

    printf("Hello from Nios II on (SDRAM)\n");

    while(1) {

    	IOWR_ALTERA_AVALON_PIO_DATA(LED_BASE,   0x05);

    }

    void *lots_of_memory = malloc(1024*1024*10); // lets allocate 10MB or memory
    printf("malloc returned 0x%08lx\n", (alt_u32)lots_of_memory);

  return 0;
}

